# AWS Configuration
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_DEFAULT_REGION=us-east-1

# S3 Configuration
S3_BUCKET_NAME=adpa-data-bucket

# API Configuration
FLASK_ENV=development
FLASK_SECRET_KEY=your_secret_key_here
